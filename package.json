{
  "name": "start_template",
  "private": true,
  "version": "1.0.0",
  "description": "a starter Template",
  "main": "index.js",
  "scripts": {
    "postinstall": "cd client && npm install",
    "build": "cd client && npm run build",
    "test:watch": "cd client && npm run test:watch",
    "test": "npm run lint && npm run prettier && cd client && npm test",
    "lint": "eslint . --ext .js",
    "prettify": "prettier --write .",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "cd client && npm start",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx,md,mdx,html,css,json}\"",
    "server": "nodemon server.js",
    "start": "node server.js"
  },
  "dependencies": {
    "@types/leaflet": "^1.7.11",
    "classnames": "^2.3.1",
    "cloudinary": "^1.23.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "immer": "^9.0.6",
    "init": "^0.1.2",
    "mongodb": "^4.3.0",
    "postinstall": "^0.7.0",
    "prettier": "^2.7.1",
    "ssri": "^8.0.1"
  },
  "devDependencies": {
    "@types/leaflet": "^1.7.0",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-leaflet": "^2.8.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-eslint": "^10.1.0",
    "concurrently": "^7.0.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "nodemon": "^2.0.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lauravikanis/start_template.git"
  },
  "author": "Laura Vikanis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lauravikanis/start_template/issues"
  },
  "homepage": "https://github.com/lauravikanis/start_template#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
